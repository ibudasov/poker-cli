{
  "name": "ibudasov/poker-cli",
  "description": "Command Line Interface application for the famous ibudasov/poker",
  "require": {
    "php": ">=7.2",
    "ibudasov/poker": "dev-master",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "PokerCli\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "scripts": {
    "example": "php index.php --inputFile exampleInputFile.txt"
  },
  "license": "MIT",
